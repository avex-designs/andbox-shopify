.login-labels {
  margin: 20px 5px;
  position:relative;
  display:inline-block;
}

.login-labels-span {
  padding:10px;
  padding-left: 0px;
  pointer-events: none;
  position:absolute;
  left:0;
  top:0;
  transition: 0.2s;
  transition-timing-function: ease;
  transition-timing-function: cubic-bezier(0.25, 0.1, 0.25, 1);
  opacity:0.5;
}



.login-inputs:focus + span, .login-inputs:not(:placeholder-shown) + span {
  opacity:1;
  transform: scale(0.75) translateY(-100%) translateX(-15px);
}

/* For IE Browsers*/
.login-inputs:focus + span, .login-inputs:not(:-ms-input-placeholder) + span {
  opacity:1;
  transform: scale(0.75) translateY(-100%) translateX(-15px);
}


